{
  "name": "enum-converter",
  "version": "1.3.6",
  "description": "Convert Enums from one language to another",
  "keywords": [
    "enum-converter",
    "enum",
    "enums",
    "converter",
    "transpiler",
    "enumc"
  ],
  "author": "Nitzan.o <this.vptr@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nitzano/enum-converter.git"
  },
  "bugs": {
    "url": "https://github.com/nitzano/enum-converter/issues"
  },
  "main": "./build/index.js",
  "typings": "./build/index.d.ts",
  "bin": {
    "enumc": "./build/src/cli/cli.js"
  },
  "files": [
    "CHANGELOG",
    "README.md",
    "LICENSE",
    "build/"
  ],
  "scripts": {
    "prebuild": "yarn run clean:soft",
    "build": "tsc",
    "build:watch": "yarn run build -w",
    "build:prod": "yarn run build -p tsconfig.prod.json",
    "cli": "ts-node ./src/cli/cli.ts",
    "clean": "rimraf build",
    "clean:soft": "rimraf build/*",
    "predebug": "yarn run build",
    "debug": "node --inspect-brk=0.0.0.0:9229 --nolazy ./build/src/cli/cli.js",
    "lint": "tslint -p tsconfig.json",
    "lint:fix": "yarn run lint -- --fix",
    "test": "jest",
    "test:debug": "node --inspect-brk=0.0.0.0 ./node_modules/.bin/jest -i",
    "test:watch": "jest --watch",
    "dev": "yarn run build:watch"
  },
  "dependencies": {
    "antlr4ts": "^0.4.1-alpha.0",
    "change-case": "4.1.2",
    "estree-walker": "0.5.1",
    "filbert": "0.1.20",
    "java-ast": "^0.1.0",
    "lodash": "^4.17.11",
    "tmp": "0.0.33",
    "typescript": "4.1.5",
    "yargs": "11.0.0"
  },
  "devDependencies": {
    "@types/estree": "0.0.38",
    "@types/jest": "^24.0.6",
    "@types/lodash": "4.14.106",
    "@types/node": "8.10.7",
    "@types/tmp": "0.0.33",
    "@types/yargs": "16.0.0",
    "jest": "^24.1.0",
    "prettier": "1.12.1",
    "rimraf": "2.6.2",
    "ts-jest": "^24.0.0",
    "ts-node": "7.0.1",
    "tslint": "5.9.1",
    "tslint-config-prettier": "1.12.0",
    "tslint-plugin-prettier": "1.3.0"
  }
}
